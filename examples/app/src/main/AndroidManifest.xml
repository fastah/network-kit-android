<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.getfastah.networkkit.testapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.getfastah.exampleswithfastahnetworkkit.DemoAudioVideoPreloadActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!-- Replace the android:value with the application name, usually the same as package name of the app -->
        <meta-data android:name="com.getfastah.networkkit.MeasureConfig.ApplicationName"
            android:value="YOUR_APPLICATION_ID" />
        <!-- Replace the android:value with the key provided to you by Fastah support (support@getfastah.com) for this application ID. -->
        <meta-data android:name="com.getfastah.networkkit.MeasureConfig.ApplicationKey"
            android:value="YOUR_APPLICATION_KEY" />
    </application>
</manifest>
